{
    "name": "vcl-extension",
    "displayName": "vcl_extension",
    "description": "An extension to highlight VCL Syntax",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "vcl",
            "aliases": ["VCL", "vcl"],
            "extensions": [".vcl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vcl",
            "scopeName": "sourcy.vcl",
            "path": "./syntaxes/vcl.tmLanguage.json"
        }]
    }
}