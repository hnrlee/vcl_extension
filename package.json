{
    "name": "vcl-extension",
    "displayName": "vcl_extension",
    "description": "An extension to highlight VCL Syntax",
    "version": "1.0.0",
    "publisher": "leeh",
    "repository": "https://github.com/hnrlee/vcl_extension",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages","Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "vcl",
            "aliases": ["VCL", "vcl"],
            "extensions": [".vcl",".VCL"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vcl",
            "scopeName": "source.vcl",
            "path": "./syntaxes/vcl.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "vcl",
                "path": "./snippets.json"
            }
        ]
    }
}